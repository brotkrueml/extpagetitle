{
    "name": "brotkrueml/extpagetitle",
    "description": "Easy setting of the browser page title in TYPO3 extensions",
    "license": "GPL-2.0-or-later",
    "type": "typo3-cms-extension",
    "keywords": [
        "TYPO3",
        "extension",
        "page title",
        "seo"
    ],
    "authors": [
        {
            "name": "Chris MÃ¼ller",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/brotkrueml/extpagetitle",
    "support": {
        "issues": "https://github.com/brotkrueml/extpagetitle/issues",
        "docs": "https://docs.typo3.org/p/brotkrueml/extpagetitle/main/en-us/"
    },
    "require": {
        "php": ">= 7.4",
        "typo3/cms-core": "^11.5 || ^12.4 || ^13.2"
    },
    "require-dev": {
        "brotkrueml/coding-standards": "~3.0.0",
        "ergebnis/composer-normalize": "~2.43.0",
        "php-coveralls/php-coveralls": "^2.7",
        "phpunit/phpunit": "^9.6"
    },
    "autoload": {
        "psr-4": {
            "Brotkrueml\\Extpagetitle\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Brotkrueml\\Extpagetitle\\Tests\\": "Tests"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        },
        "bin-dir": ".Build/bin",
        "sort-packages": true,
        "vendor-dir": ".Build/vendor"
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.4.x-dev"
        },
        "typo3/cms": {
            "app-dir": ".Build",
            "extension-key": "extpagetitle",
            "web-dir": ".Build/web"
        }
    }
}
